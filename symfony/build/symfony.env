PACKS="git wget apache2 unzip curl vim iproute2 net-tools"

#Comandos a ejecutar al crear la imagen separados por ","
COMMANDS="export DEBIAN_FRONTEND=noninteractive ; apt install -y php php-cli php-xml,curl -sS https://getcomposer.org/installer -o /tmp/composer-setup.php,php /tmp/composer-setup.php --install-dir=/usr/local/bin --filename=composer,wget https://get.symfony.com/cli/installer -O - | bash,mv /root/.symfony5/bin/symfony /usr/bin,git config --global user.email symfony@example.com,git config --global user.name symfony_project"

